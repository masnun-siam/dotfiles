{
  "dart.devToolsBrowser": "default",
  "dart.previewSdkDaps": true,
  "dart.flutterSdkPath": "~/Tools/flutter/",
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "xml.format.maxLineWidth": 80,
  "xml.format.splitAttributes": true,
  "explorer.compactFolders": false,
  "editor.fontFamily": "SFMono Nerd Font",
  "editor.fontSize": 18,
  "editor.bracketPairColorization.enabled": true,
  "zenMode.fullScreen": true,
  "editor.wordWrapColumn": 90,
  "editor.matchBrackets": "always",
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.surround": true,
  "vim.hlsearch": true,
  "vim.foldfix": true,
  "vim.incsearch": true,
  "vim.leader": " ",
  "editor.cursorBlinking": "solid",
  "editor.smoothScrolling": true,
  "editor.renderLineHighlight": "gutter",
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.external.osxExec": "Alacritty.app",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.colorCustomizations": {
    "editorHoverWidget.border": "#888888",
    "[Solarized Dark Vim]": {
      // "editor.background": "#000000"
    },
    "[Solarized Dark]": {
      "editor.background": "#111111"
    }
  },
  "vim.handleKeys": {
    "<C-q>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j","j"],
      "after": ["<esc>"]
    }
  ],
  
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "a"],
      "commands": ["keyboard-quickfix.openQuickFix"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"],
    },
    {
      "before": ["leader", "x"],
      "commands": ["workbench.action.closeActiveEditor"],
    },
    {
      "before": ["s", "f"],
      "commands": ["file-browser.open"]
    },
    {
      "before": ["<leader>", "f", "w"],
      "commands": ["workbench.action.showAllSymbols"],
      "args": ""
    },
    {
      "before" : ["<leader>", "f", "t"],
      "commands" : ["fuzzySearch.activeTextEditor"]
    },
    {
      "before": [";", "F"],
      "commands": ["Format Buffer"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["extension.jumpy-word"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["extension.jumpy-line"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "after": ["g", "h"],
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.problems.focus"],
    },
    {
      "before": ["g", "p"],
      "commands": ["editor.action.peekDefinition"],
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"],
    },
    {
      "before": ["<leader>","j"],
      "commands": ["editor.action.marker.nextInFiles"],
    },
    {
      "before": ["<leader>","k"],
      "commands": ["editor.action.marker.previousInFiles"],
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "a"],
      "commands": ["keyboard-quickfix.openQuickFix"]
    },
  ],
  "dart.runPubGetOnPubspecChanges": "always",
  "files.autoSave": "off",
  "editor.tabSize": 2,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLensFontFamily": "Fira Sans",
  "editor.codeLensFontSize": 17,
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.enabled": false,
  "editor.guides.bracketPairs": "active",
  "editor.minimap.enabled": false,
  "workbench.settings.editor": "json",
  "vspacecode.bindingOverrides": [
    {
      "keys": "c.a",
      "name": "Code Action",
      "type": "command",
      "command": "keyboard-quickfix.openQuickFix",
      "icon": "lightbulb"
    },
    {
      "keys": "c.f",
      "name": "Format Buffer",
      "type": "command",
      "icon": "settings",
      "command": "editor.action.format"
    }
  ],
  "vim.enableNeovim": true,
  "workbench.iconTheme": "material-icon-theme",
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "git.confirmSync": false,
  "git.autofetch": true,
  "workbench.startupEditor": "none",
  "oneDarkPro.vivid": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "ios/.symlinks": true,
    "build": true,
    "android/.gradle": true
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "ios/.symlinks": true,
    "build": true,
    "android/.gradle": true,
    "**/.dart_tool": true,
    "**/.plugins": true,
    "**/.metadata": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true
  },
  "advancedNewFile.exclude": {
    "**/.dart_tool": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "ios/.symlinks": true,
    "build": true,
    "android/.gradle": true
  },
  "autoHide.autoHidePanel": false,
  "workbench.layoutControl.enabled": false,
  "editor.renderWhitespace": "none",
  "redhat.telemetry.enabled": true,
  "task.problemMatchers.neverPrompt": {
    "shell": true
  },
  "dart_data_class_generator.fromJson.enabled": false,
  "dart_data_class_generator.toJson.enabled": false,
  "dart_data_class_generator.fromMap.default_values": true,
  "breadcrumbs.enabled": false,
  "fzf-quick-open.ripgrepSearchStyle": "Ignore case",
  "dart.warnWhenEditingFilesInPubCache": false,
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "One Dark Pro Darker",
}
