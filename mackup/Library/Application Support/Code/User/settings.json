{
  "explorer.compactFolders": false,
  "editor.fontFamily": "FiraCode Nerd Font, SFMono Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 20,
  "editor.fontWeight": "300",
  "editor.wordWrap": "on",
  "terminal.integrated.fontSize": 15,
  "prettier.tabWidth": 2,
  "editor.tabSize": 2,
  "oneDarkPro.vivid": false,
  "workbench.list.smoothScrolling": true,
  "keyboard.dispatch": "keyCode",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.external.osxExec": "/System/Volumes/Data/Applications/iTerm.app",
  "editor.cursorBlinking": "solid",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.indentation": false,
  "editor.renderLineHighlight": "gutter",
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.lineHeight": 2.0,
  "dart.previewFlutterUiGuides": true,
  "dart.flutterRememberSelectedDevice": true,
  "dart.documentation": "summary",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "workbench.startupEditor": "none",
  "files.autoSave": "afterDelay",
  "editor.cursorWidth": 2,
  "workbench.settings.editor": "json",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffff00",
    "editorCursor.background": "#000000",
    "editorHoverWidget.border": "#888888"
  },
  "workbench.iconTheme": "material-icon-theme",
  "security.workspace.trust.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "redhat.telemetry.enabled": true,
  "explorer.confirmDelete": false,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["vspacecode.space"]
    },
    {
      "before": [","],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["vspacecode.space"]
    },
    {
      "before": [","],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    }
  ],
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vim.enableNeovim": true,
  "dart.runPubGetOnPubspecChanges": "prompt",
  "dart.hotReloadOnSave": "manual",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.statusBarColorControl": false,
  "vspacecode.bindingOverrides": [
    {
      "keys": ["s", "t"],
      "name": "Ripgrep",
      "type": "command",
      "command": "extension.ripgrep"
    },
    {
      "keys": ["a"],
      "name": "Quick Fix",
      "type": "command",
      "command": "vim.remap",
      "args": {
        "commands": [
          // { "command": ":noh" },
          {
            "command": "keyboard-quickfix.openQuickFix"
            // "args": { "kind": "refactor.extract" }
          }
        ]
      }
    }
  ],
  "go.toolsManagement.autoUpdate": true,
  "advancedNewFile.exclude": {
    "**/linux": true,
    "**/macos": true,
    "**/windows": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "**/build": true,
    "**/.dart_tool": true,
    "**/.symlinks": true,
    "**/.Pods": true,
    "**/.gradle": true
  },
  "search.exclude": {
    "**/linux": true,
    "**/macos": true,
    "**/windows": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "**/build": true,
    "**/.dart_tool": true,
    "**/.symlinks": true,
    "**/.Pods": true,
    "**/.gradle": true
  },
  "files.exclude": {
    "**/linux": true,
    "**/macos": true,
    "**/windows": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "terminal.explorerKind": "external",
  "xml.format.splitAttributes": false,
  "git.openRepositoryInParentFolders": "never",
  "workbench.colorTheme": "Shuvo Deb Color Theme V-0.1",
  "workbench.activityBar.visible": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "cSpell.userWords": [
    "Autovalidate",
    "Cupertino",
    "easymotion",
    "esbenp",
    "firestore",
    "neovim",
    "nvim",
    "Pubspec",
    "quickfix",
    "RGBO",
    "vspacecode",
    "whichkey"
  ],
  "git.confirmSync": false
}
